{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/(with-auth-layout)/doctor-consult/page.jsx", "app/(with-auth-layout)/live-metrics/page.jsx", "app/api/live-metrics/route.js", "app/(with-auth-layout)/booking/page.jsx", "app/(with-auth-layout)/medical-history/page.jsx", "app/(with-auth-layout)/health-insights/page.JSX", "app/(with-auth-layout)/recomendation/page.jsx", "lib/health-analysis.js", "app/(with-auth-layout)/iot/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
